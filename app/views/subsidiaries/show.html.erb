<h2><%= @subsidiary.name %></h2>
<dl>
  <dt>CNPJ:</dt> 
  <dd><%= @subsidiary.cnpj %></dd>
  <dt>Endereço:</dt> 
  <dd><%= @subsidiary.address %></dd>
</dl>

<%= link_to  "Editar", edit_subsidiary_path(@subsidiary) %>
<%= link_to  "Deletar", subsidiary_path(@subsidiary), method: :delete, 
                        data: {confirm: 'Você tem certeza?'}%>
<%= link_to  "Voltar", root_path %>