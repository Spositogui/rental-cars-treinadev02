<h3>Locações</h3>
<p><%= link_to  "Agendar locação", new_rental_path %></p>

<%= form_with(url:search_rentals_path, local: true, method: :get) do %>
  <%= label_tag :q, 'Código' %>
  <%= text_field_tag :q %>
  <%= submit_tag 'Buscar' %>
<% end %>

<!-- render partial: 'rental', collection: @rentals -->
<!-- render @rentals -->
<ul>
  <% @rentals.each do |rental| %>
    <li><%= link_to rental.reservation_code, rental %></li>  
  <% end %>
</ul>


  


